<aside class="sidebar-section">
    <div class="sidebar-container">
        <a href="/" class="home-link">
            Home
            <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="32" height="32" viewBox="0 0 24 24">
                <path d="M 12 2 A 1 1 0 0 0 11.289062 2.296875 L 1.203125 11.097656 A 0.5 0.5 0 0 0 1 11.5 A 0.5 0.5 0 0 0 1.5 12 L 4 12 L 4 20 C 4 20.552 4.448 21 5 21 L 9 21 C 9.552 21 10 20.552 10 20 L 10 14 L 14 14 L 14 20 C 14 20.552 14.448 21 15 21 L 19 21 C 19.552 21 20 20.552 20 20 L 20 12 L 22.5 12 A 0.5 0.5 0 0 0 23 11.5 A 0.5 0.5 0 0 0 22.796875
                    11.097656 L 12.716797 2.3027344 A 1 1 0 0 0 12.710938 2.296875 A 1 1 0 0 0 12 2 z">
                </path>
            </svg>
        </a>
        <% if (locals.homeFolder) {%>
            <% if (locals.children) {%>
                <h3 class="home-subfolders-header">Home Folders:</h3>
                <div class="home-subfolders-container">
                <% children.forEach(folder => { %>
                    <a href="/folder/library/<%= folder.id %>" class="sub-home-folder-link">
                        <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="30" height="20" viewBox="0 0 24 24">
                            <path fill="#000000" d="M20,6h-8l-1.414-1.414C10.211,4.211,9.702,4,9.172,4H4C2.9,4,2,4.9,2,6v12c0,1.1,0.9,2,2,2h16c1.1,0,2-0.9,2-2V8 C22,6.9,21.1,6,20,6z"></path>
                        </svg>
                        <%= folder.name %>
                    </a>
                <% }) %>
                </div>
            <% } %>
            <a href="/folder/create/<%= homeFolder.id %>" class="add-folder-link">
                <p>New Folder</p>
                <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="20" height="20" viewBox="0 0 24 24">
                    <path d="M12,2C6.486,2,2,6.486,2,12s4.486,10,10,10s10-4.486,10-10S17.514,2,12,2z M16,13.25h-2.75V16c0,0.69-0.56,1.25-1.25,1.25 s-1.25-0.56-1.25-1.25v-2.75H8c-0.69,0-1.25-0.56-1.25-1.25S7.31,10.75,8,10.75h2.75V8c0-0.69,0.56-1.25,1.25-1.25 S13.25,7.31,13.25,8v2.75H16c0.69,0,1.25,0.56,1.25,1.25S16.69,13.25,16,13.25z"></path>
                </svg>
            </a>
        <% } %>
        
        <a href="/file/upload/<%= homeFolder.id %>" class="upload-home-file-link">
            Upload File
            <svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" fill="#000000" width="20px" height="20px"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <g> 
                <path d="M3,12.3v7a2,2,0,0,0,2,2H19a2,2,0,0,0,2-2v-7" fill="none" stroke="#000000" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"></path> <g> <polyline data-name="Right" fill="none" id="Right-2" points="7.9 6.7 12 2.7 16.1 6.7" stroke="#000000" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"></polyline> 
                    <line fill="none" stroke="#000000" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" x1="12" x2="12" y1="16.3" y2="4.8"></line> 
                </g> </g> </g> 
            </svg>
        </a>
    </div>
    <div class="logout-container">
        <a href="/user/logOut" class="logout-link">Logout</a>
    </div>
</aside>